{
    "manifest_version": 3,
    "name": "radiko Keyboard Player",
    "description": "radikoをキーで操作",
    "version": "1.1",
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://radiko.jp/*"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
